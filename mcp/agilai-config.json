{
  "mcpServers": {
    "agilai-invisible-orchestrator": {
      "command": "node",
      "args": ["-e", "require('agilai/dist/mcp/mcp/server.js')"]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-chrome-devtools"]
    },
    "shadcn": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-shadcn"]
    },
    "gitmcp": {
      "command": "npx",
      "args": ["-y", "gitmcp"]
    }
  }
}
